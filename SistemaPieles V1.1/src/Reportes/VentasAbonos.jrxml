	<queryString>
		<![CDATA[SELECT
    t1.idventa,
    t1.fechaventa,
    t2.descripcion,
    t1.totalventa,
    t2.abono
FROM
    venta t1
LEFT JOIN abono t2 ON
    (0)
WHERE
     t1.idcliente  = $P{idcliente}
UNION
SELECT
    t2.idabono,
    t2.fechaabono,
    t2.descripcion,
    t1.totalventa,
    t2.abono
FROM
    venta t1
RIGHT JOIN abono t2 ON
    (0)
WHERE
     t2.idcliente  = $P{idcliente}]]>
	</queryString>
